{
  "name": "Duckduckgo",
  "description": "Search Duckduckgo",
  "manifest_version": 2,
  "version": "1.2",
  "browser_specific_settings": {
    "gecko": {
      "id": "ddg@search.mozilla.org"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "@ddg",
      "name": "Duckduckgo",
      "search_url": "https://duckduckgo.com/",
      "search_form": "https://duckduckgo.com/?k1=-1&kaj=m&kak=-1&kao=-1&kaq=-1&kau=-1&kn=1&kp=-2&q={searchTerms}",
      "search_url_get_params": "k1=-1&kaj=m&kak=-1&kao=-1&kaq=-1&kau=-1&kn=1&kp=-2&q={searchTerms}",
      "suggest_url": "https://ac.duckduckgo.com/ac/",
      "suggest_url_get_params": "q={searchTerms}&type=list"
    }
  }
}
